container_commands:
  00_setup:
    command: "source /opt/python/run/venv/bin/activate && source /opt/python/current/env"
  01_migrate:
    command: "django-admin migrate --noinput"
    leader_only: true
  02_superuser:
    command: "django-admin superuser"
    leader_only: true
  03_collectstatic:
    command: "django-admin collectstatic --noinput"

option_settings:
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: "Orpose.settings.production"