container_commands:
  01_migrate:
    command: "echo -e '#!/bin/bash\ndocker exec `docker ps -a -q | head -n 1` /opt/python/run/venv/bin/python /opt/python/current/app/manage.py migrate' > /opt/elasticbeanstalk/hooks/appdeploy/post/99_migrate.sh && sudo chmod +x /opt/elasticbeanstalk/hooks/appdeploy/post/99_migrate.sh"
    leader_only: true
option_settings:
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: "Orpose.settings.production"
  aws:elasticbeanstalk:container:python:
    WSGIPath: Orpose/wsgi.py